<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dayi.follow.dao.follow.SourceReportMapper">

    <select id="findByTime" resultType="com.dayi.follow.model.follow.SourceReport" parameterType="map">
        select * from source_report where create_time between #{startDate} and #{endDate}
        <if test="null !=type">
            and type =#{type}
        </if>
    </select>

</mapper>